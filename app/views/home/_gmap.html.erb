<!-- Google Map Script -->
<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCgN5AxJ2pKmQVP4ITrog8SFSkGwmh-fAE">
</script>

<!-- Google map javascript functionality -->
<script type="text/javascript">
  function initialize() {

  	var myCenter = new google.maps.LatLng(34.0500,-118.2500);

    var mapOptions = {
      center: myCenter,
      zoom: 10
    };
    var map = new google.maps.Map(document.getElementById('map-canvas'),
        mapOptions);

    <% @users.each do |z| %>
   		var marker = new google.maps.Marker({
   			position: new google.maps.LatLng(<%= z.coordinates %>[1],<%= z.coordinates %>[0]),
   			map: map,
   			title: "<%= z.name %>"
   		});
    <% end %>

   } //End Google Map initialization

  google.maps.event.addDomListener(window, 'load', initialize);
</script>
<!-- End Google Map Script -->
<div id="map-canvas"></div>